        .TITLE  VUNODE.MDL

        .GLOBL  VURGINIT,VURGS1,VURGS0,VURGSTATE,VURGGET

TBLARG: ;   
        .WORD   160272,160312,160332,160352,160372
TBLMUX: ; MUX
        .WORD   0,20000,40000,60000,100000,120000,140000,160000

; ***************
; IN    :       -
; OUT   :       -
; ***************
VURGINIT:
	.SAVRON
	CLR     R1
	ITM.FML #VU$RG,R2
1$:	FML.ITM R2,R1,R0
	ASL	R1
	MOV     4(R0),2(R0)	;   
	MOV	2(R0),@(R0) 	;   
	ASR	R1
        INC	R1
	CMP	R1,#5
	BLT	1$
	RETURN

; ***************
; IN    :       -
; OUT   :       -
; ***************
VURGTEST:
	.SAVRON
	BIC	#TRPCALL!TRPOERROR,TRPSTS
	CLR     R1
	ITM.FML #VU$RG,R2
1$:	FML.ITM R2,R1,R0
	ASL	R1
;;;	BIC	#TRPOERROR,TRPSTS
	MOV	2(R0),@(R0) 	; -    ?
	BIC	#TRPOERROR,TRPSTS
	MOV	@(R0),R3	; -   ?
	ASR	R1
        INC	R1
	CMP	R1,#5
	BLT	1$
	CLC
	BIT	#TRPCALL,TRPSTS
	BEQ	2$
	SEC
2$:	RETURN

; *************************************************************
; IN    :       R0 (    -480  UPR)
; OUT   :       R0 (    )
; *************************************************************

VURGS1:	.SAVRON
   	MOV     #-1,R2          ; 
        BR      VURGSET
VURGS0:	.SAVRON
   	CLR     R2              ; 
VURGSET:
        MOVB    8.(R0),R1        ;  <0?
        BMI     VUBDERR            ;
        CMP     R1,#16.		;  > 16 (-   )?
        BGE     VUBDERR            ;
        MOV     #1,R3
        ASH     R1,R3           ;   

	MOV	6(R0),R4

	BIC     R3,2(R4)	;     
        TST     R2              ; ?
        BEQ     2$              ;
	BIS     R3,2(R4)	;     
2$:
        ;      
	MOV	2(R4),@(R4) 	;   
        CLC              	; 
3$:     RETURN


VUBDERR:
        MOV     #M$1000,R0
        CALL    PRINT
	SEC
	RETURN


; *************************************************************
; IN    :       R0 (      UPR)
; OUT   :       R0 (=0 -  , <>0 -  )
; *************************************************************

VURGSTATE:
	.SAVRON

        MOVB    8.(R0),R1        ;  <0?
        BMI     VUBDERR            ;
        CMP     R1,#16.        ;  > 16 (-   )?
        BGE     VUBDERR            ;
        MOV     #1,R3
        ASH     R1,R3           ;   

	MOV	6(R0),R4

	CLR	R0
        BIT     R3,2(R4)	; ?
	BEQ	3$		;
	COM	R0
3$:
	CLC
	RETURN


; *************************************************************
; IN    :       R0 (      SOST)
; OUT   :       R0 (=0 -  , <>0 -  )
; *************************************************************

VURGGET:
	.SAVRON

        MOVB    8.(R0),R1        ;  <0?
        BMI     VUBDERR            ;
        CMP     R1,#16.        ;  > 16 (-   )?
        BGE     VUBDERR        ;
        MOV     #1,R3
        ASH     R1,R3           ;   

	MOV	6(R0),R4

        MOVB    9.(R0),R5
	CMP	R5,#-1		; MUX ?
        BEQ     2$              ;
        CMP     R5,#8.          ;-  MUX > 8?
        BGT     VUBDERR            ;
        ASL     R5

        ;   MUX
        BIC     #160000,2(R4)	;     160332
        BIS     TBLMUX(R5),2(R4)
        MOV     2(R4),@(R4)
2$:
	MOV	@(R4),R2	;  
	CLR	R0
        BIT     R3,R2		; ?
	BEQ	3$		;
	COM	R0
3$:
	CLC
	RETURN

M$1000: .ASCII  <12><15><11><16>/    "/<0>

PRINT:  .PRINT
        RETURN

