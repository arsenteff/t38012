	.TITLE TSTRUN.MDL

TNUM::	.WORD

;************************************
.ITEM TSTRUN,,,<>,< >
;************************************
	MOV	R1,-(SP)
	MOV	R2,-(SP)
	MOV	R5,-(SP)
	MOV	R0,-(SP)
	STR.OUT 		; 
	MSG.TXT @SP		; 
	MSG.TXT #MS$TNUM,@SP	; 
;;;	MSG.TXT #TSTRUN 	; !
	MOV	@SP,R0
	MOV	(R0)+,TNUM	;  
	MOV	(R0)+,R2	;  ?
	BEQ	2$		;
	ITM.FML @SP,R0
	MOV	R0,R1		;  
	FML.CNT R0,R5		;-   > 0?
	BLE	1$		;
	FML.FIR R1,R1
10$:
	TST	(R1)		;  
	BEQ	3$
	MSG.TXT @R1		;  
	FML.LST R2,(R1)+	;  
	SOB	R5,10$
	MSG.TXT #MS$TEND
	CLC
	BR	4$
1$:	MSG.ERR #ER$TMDL
	BR	3$
2$:	MSG.ERR #ER$TDAT
3$:	CLC
4$:	MOV	(SP)+,R0
	MOV	(SP)+,R5
	MOV	(SP)+,R2
	MOV	(SP)+,R1
	RETURN
