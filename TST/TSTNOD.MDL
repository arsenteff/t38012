	ASH	R4,R0
	MOV	R0,DANNY

	MOV	2(R2),ADRES
	CALL	OUTPUT
	MOV	R3,R0
	CALL	VURGGET

	CMP	R1,#X0111		;  XP1:11 -    -1096
	BEQ	4$

