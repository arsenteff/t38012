.ITEM TSTLG0,,$NDLOG,<>,<   "0">
;********************************
	.SAVRON
	MOV	R0,R1

	CMP	@R1,#NODLOG		;  ?
	BNE	3$			;

	CMP	12(R1),#2		; ?
	BNE	3$			;

