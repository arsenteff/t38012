
NDCURRENT:	.WORD	;          
NDREPEAT:	.WORD	;        

.ITEM MS$ZAM,,,<.MSG <STRING>,NULL>,<     >
.ITEM MS$RDY,,,<.MSG <STRING>,NODNAM>,<:>
.ITEM MS$TZM,,,<.MSG <STRING>,NODNAM>,<:>

LSTNOD:
	MOV	R0,NDCURRENT

	CALL	LSTNS1			;   
	BCS	5$			;  -  

	FML.LST #$NDNOD,#ZAMNOD
	FML.LST #$NDJMP,#ZAMJMP
	FML.LST #$NDLOG,#ZAMLOG

	MSG.TXT	#MS$RDY,NDCURRENT
5$:	CLC
	RETURN


;     "1"   "0"
LSTNS1:
	MOV	R1,-(SP)
	MOV	NDCURRENT,R1

	FML.LST #$NDNOD,#NDSET1
	FML.LST #$NDJMP,#JPSET1
	FML.LST #$NDLOG,#RGSET1

	MOV	R1,R0
	CMP	@R1,#NODJMP		;  ?
	BNE	3$			;
	CALL	JPSET0
	BR	2$
3$:
	CMP	@R1,#NODNOD		;  ?
	BNE	4$			;
	CALL	NDSET0
	BR	2$
4$:
	CMP	@R1,#NODLOG		;   -?
	BNE	5$			;
	CALL	RGSET0
2$:
	CALL	NDZADR
	CLC
	BR	6$
5$:
	SEC
6$:	MOV	(SP)+,R1
	RETURN

TZMNUM:	.WORD	40

;********************************
.ITEM TSTZAM,,,<>,<     >
;********************************
	.SAVRON

	STR.OUT 		; 
	MSG.TXT #TSTZAM		; 
	MSG.TXT #MS$TNUM,#TZMNUM; 

	CLR	STATUS
	CALL	OSVOB
	CALL	ZAHVAT

	FML.LST #$NDX02,#LSTNOD
	FML.LST #$NDX01,#LSTNOD
	FML.LST #$NDX03,#LSTNOD
	FML.LST #$NDX04,#LSTNOD
	FML.LST #$NDX05,#LSTNOD
	FML.LST #$NDX06,#LSTNOD

	MSG.TXT #MS$TEND
	RETURN

;************************
;  
;************************
NDSET0:
	CMP	R0,#X0213A		; ?
	BEQ	3$			;
	MOV	6.(R0),R0		;UPR
	CALL	VURGS0
3$:	RETURN

;*************************
;  
;*************************
NDSET1:
	CMP	R0,#X0213A		; ?
	BEQ	3$			;
	MOV	6.(R0),R0		;UPR
	CALL	VURGS1
3$:	CLC
	RETURN

;************************
;  
;************************
JPSET0:
	;  SOST
	TST	8.(R0)			;SOST?
	BEQ	3$			;
	MOV	16(R0),R0
3$:
	MOV	6.(R0),R0		;UPR
	CALL	VURGS0
	RETURN

;*************************
;  
;*************************
JPSET1:
	;  SOST
	TST	8.(R0)			;SOST?
	BEQ	3$			;
	MOV	16(R0),R0
3$:
	MOV	6.(R0),R0		;UPR
	CALL	VURGS1
	CLC
	RETURN



;********************************
;    
;********************************
RGSET1:
	.SAVRON
	MOV	R0,R1

	CMP	@R1,#NODLOG		;  ?
	BNE	3$			;

	CMP	12(R1),#2		; ?
	BNE	3$			;

	MOV	14(R1),R2

	CMP	@R2,#3			; -?
	BNE	6$			;
	CLR	DANNY
	MOV	2(R2),ADRES
	CALL	OUTPUT
	BR	8$
6$:
	CMP	@R2,#1			; ?
	BNE	5$			;
8$:
	MOV	6.(R1),R3		;UPR?
	BEQ	3$			;

	MOV	R1,R0
	CALL	NODS.1

	BR	3$
5$:
	CMP	@R2,#2			; ?
	BNE	3$			;

	MOV	8.(R1),R3		;SOST?
	BEQ	3$			;

	CLR	R5
;	TSTB	17(R1)			;  ?
;	BEQ	7$			;
;	MOV	#1,R5
;	MOVB	16(R0),R4
;	ASH	R4,R5
7$:
;	CALL	ZAHVAT

	CALL	RGSTOUT
3$:
	CLC
	RETURN

RGSTOUT:
	MOV	R5,DANNY
	MOV	2(R2),ADRES
;;;	MOV	RG$RGG+2,-(SP)		;     
;;;	MOV	RG$RGD+2,-(SP)
	CALL	OUTPUT
;;;	MOV	(SP)+,RG$RGD+2		;          Q-BUS
;;;	MOV	(SP)+,RG$RGG+2
;;;	MOV	RG$RGG+2,@RG$RGG	
;;;	MOV	RG$RGD+2,@RG$RGD
	RETURN

;********************************
;    
;********************************
RGSET0:
	.SAVRON
	MOV	R0,R1

	CMP	@R1,#NODLOG		;  ?
	BNE	3$			;

	CMP	12(R1),#2		; ?
	BNE	3$			;

	MOV	14(R1),R2

	CMP	@R2,#3			; -?
	BEQ	6$			;
	CMP	@R2,#1			; ?
	BNE	5$			;
6$:
	MOV	6.(R1),R3		;UPR?
	BEQ	3$			;

	MOV	R1,R0
	CALL	NODS.0

	BR	3$
5$:
	CMP	@R2,#2			; ?
	BNE	3$			;

	MOV	8.(R1),R3		;SOST?
	BEQ	3$			;

;	MOV	#177777,R5
;	TSTB	17(R1)			;  ?
;	BNE	7$			;

	MOV	#1,R5
	MOVB	16(R0),R4
	ASH	R4,R5
7$:
;	CALL	ZAHVAT
	CALL	RGSTOUT

3$:
	RETURN



;***********************************
;    
;***********************************
ZAMNOD:
	.SAVRON
	MOV	R0,R1
10$:
	CMP	@R1,#NODNOD		;  ?
	BNE	3$			;

	CMP	R1,NDCURRENT		;  - ?
	BEQ	3$			;

	;  SOST
	MOV	8.(R1),R2		;SOST?
	BEQ	3$			;
	CMP	R1,#X0213A		; ?
	BEQ	3$			;

	MOV	R2,R0
	CALL	VURGGET
	TST	R0
	BNE	3$

	CALL	ERRORINC

	;  
	CALL	ZAMOUT
	BC	4$			;? 
	CALL	LSTNS1
	BR	10$
3$:
	CLC
4$:	RETURN


;***********************************
;    
;***********************************
ZAMJMP:
	.SAVRON
	MOV	R0,R1
10$:
	CMP	@R1,#NODJMP		;  ?
	BNE	3$			;

	CMP	R1,NDCURRENT		;  - ?
	BEQ	3$			;
	CMP	16(R1),NDCURRENT	;  - ?
	BEQ	3$			;

	;  SOST
	MOV	8.(R1),R0		;SOST?
	BNE	6$			;
	MOV	16(R1),R0
	MOV	8.(R0),R0		;UPR
6$:
	CALL	VURGGET
	TST	R0
	BNE	3$

	CALL	ERRORINC

	;  
	CALL	ZAMOUT
	BCC	4$			;? 
	CALL	LSTNS1
	BR	10$
3$:
	CLC
4$:	RETURN

;********************************
;    -  
;********************************
ZAMLOG:
	.SAVRON
	CMP	R0,NDCURRENT		;  - ?
	BEQ	1$			;

	MOV	R0,R1
10$:
	CMP	@R1,#NODLOG		;  ?
	BNE	3$			;

	CMP	12(R1),#2		; ?
	BNE	3$			;

;	CALL	ZAHVAT

	MOV	14(R1),R2

	CMP	@R2,#3			; -?
	BEQ	8$			;
	CMP	@R2,#1			; ?
	BNE	5$			;
8$:
	MOV	6.(R1),R3		;UPR?
	BEQ	3$			;

	MOV	RG$RGG+2,-(SP)		;     
	MOV	RG$RGD+2,-(SP)
	MOV	2(R2),ADRES
	CALL	INPUT
	MOV	(SP)+,RG$RGD+2		;          Q-BUS
	MOV	(SP)+,RG$RGG+2
	MOV	RG$RGG+2,@RG$RGG	
	MOV	RG$RGD+2,@RG$RGD

	MOV	#1,R0
	MOVB	16(R1),R4
	ASH	R4,R0

	BIT	R0,DANNY
	BNE	5$

	CALL	ERRORINC

	;  
	CALL	ZAMOUT
	BCC	1$			;? 
	CALL	LSTNS1
	BR	10$

5$:
	CMP	@R2,#2			; ?
	BNE	1$			;

	MOV	8.(R1),R3		;SOST?
	BEQ	3$			;

	MOV	R3,R0
	CALL	VURGGET

	CMP	R1,#X0111		;  XP1:11 -    -1096
	BEQ	4$

	TSTB	17(R1)			;  ?
	BNE	4$			;
	TST	R0
	BNE	1$
	BR	2$
4$:
	TST	R0
	BEQ	1$
	CALL	ZAMOU2
	BCS	10$
	BR	1$
2$:
	CALL	ERRORINC

	;  
	CALL	ZAMOUT
	BCC	1$			;? 
	CALL	LSTNS1
	BR	10$
1$:
	CLC
	RETURN
3$:
	SEC
	RETURN


ZAMOUT:
	STR.OUT
	MSG.TXT	#TSTZAM
	MSG.TXT	#MS$TZM,NDCURRENT

	MSG.ERR #ER$THIG
	MSG.ERR	#MS$ZAM
	MOV	R1,R0
	CALL	ND$PR1			;   
	RETURN

ZAMOU2:
	STR.OUT
	MSG.TXT	#TSTZAM
	MSG.TXT	#MS$TZM,NDCURRENT

	MSG.ERR #ER$TLOW
	MSG.ERR	#MS$ZAM
	MOV	R1,R0
	CALL	ND$PR1			;   
	RETURN
