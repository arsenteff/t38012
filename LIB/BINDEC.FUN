	.TITLE BINDEC
;****************************************************
;      
;****************************************************
; IN  <R0> - 
; OUT <R0> -  

BINDEC: JSR	R5,SAVRON	; 
	MOV	#5,R4		;  
	MOV	#BUFDEC,R3
	ADD	R4,R3
	MOVB	#'.,@R3 	; -   
	MOVB	#200,1(R3)	; 
1$:	MOV	R0,R1
	CLR	R0		;   
	DIV	#10.,R0 	;  10.   
	ADD	#60,R1		; 
	MOVB	R1,-(R3)	;    
	SOB	R4,1$
	MOV	R3,R0
	RETURN
