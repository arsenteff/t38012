	.TITLE BINDEC
;****************************************************
;      
;****************************************************
; IN  <R0> - 
; OUT <R0> -  

BINHEX: JSR	R5,SAVRON	; 
	MOV	#4,R4		; 
	MOV	#BUFLIB,R3
	ADD	R4,R3
	MOVB	#'&,@R3 	; -   
	MOVB	#200,1(R3)	; 
1$:	MOV	R0,R1
	CLR	R0		;   
	DIV	#16.,R0 	;  8.   
	ADD	#60,R1		; 
	CMP	R0,#10.
	BLT	.+6
	ADD	#7,R1
	MOVB	R1,-(R3)	;    
	SOB	R4,1$
	MOV	R3,R0
	RETURN
